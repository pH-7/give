{
  "name": "giveit",
  "version": "2.0.1",
  "description": "Debug with ease any nested objects, arrays, etc. JSON.stringify wrapper to display nested objects with indentation & color for quick debugging.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "pre-publish": "npm run build",
    "test": "ava"
  },
  "keywords": [
    "stringify",
    "debug",
    "debugging",
    "nested",
    "objects",
    "arrays",
    "json",
    "indentation",
    "color",
    "pretty",
    "print"
  ],
  "ava": {
    "files": [
      "tests/**/*.test.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "author": "Pierre-Henry Soria",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pH-7/giveit.git"
  },
  "homepage": "https://github.com/pH-7/giveit#readme",
  "bugs": {
    "url": "https://github.com/pH-7/giveit/issues"
  },
  "dependencies": {
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "@types/node": "^16.18.97",
    "ava": "^6.1.3",
    "ts-node": "^10.9.2",
    "typescript": "^4.7.4"
  }
}
